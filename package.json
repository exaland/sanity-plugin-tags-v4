{
  "name": "sanity-plugin-tags-v4",
  "version": "1.0.6",
  "description": "A multi-tag input for Sanity Studio",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/exaland/sanity-plugin-tags-v4#readme",
  "bugs": {
    "url": "https://github.com/exaland/sanity-plugin-tags-v4/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/exaland/sanity-plugin-tags-v4.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "react-select": "^5.10.2"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.2.13",
    "@sanity/plugin-kit": "^3.1.6",
    "@types/react": "^19.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^7.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^19.0",
    "react-dom": "^19.0",
    "react-is": "^19.0",
    "rimraf": "^4.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^19.0",
    "react-dom": "^19.0",
    "rxjs": "^7.8.0",
    "sanity": "^5.5.0",
    "styled-components": "^6.1.0"
  },
  "engines": {
    "node": ">=14"
  },
  "authors": [
    "Alexandre Magnier | Exaland Concept <exaland@gmail.com>",
    "P Christopher Bowers <pcbowers@gmail.com>",
    "Leo Baker-Hytch <l.bakerhytch@gmail.com>"
  ],
  "overrides": {
    "esbuild": "0.25.9"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "tsc": false
    }
  },
  "sanityExchangeUrl": "https://www.sanity.io/plugins/tags"
}
